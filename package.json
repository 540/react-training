{
  "name": "react-training",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "formik": "2.1.5",
    "lodash": "4.17.20",
    "polished": "3.6.7",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router-dom": "5.2.0",
    "react-scripts": "3.4.3",
    "styled-components": "5.2.0",
    "tiny-emitter": "2.1.0",
    "yup": "0.29.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom-fourteen",
    "test:e2e": "BROWSER=none CI=true run-p start cypress --race",
    "test:precommit": "cross-env CI=true run-p test test:e2e",
    "eject": "react-scripts eject",
    "storybook": "NODE_PATH=src start-storybook -p 3022 -c .storybook -s storybook",
    "cypress": "cypress run"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@storybook/addon-actions": "6.0.22",
    "@storybook/addon-knobs": "6.0.22",
    "@storybook/addon-viewport": "6.0.22",
    "@storybook/addons": "6.0.22",
    "@storybook/react": "6.0.22",
    "@testing-library/react": "11.0.4",
    "babel-loader": "8.1.0",
    "cross-env": "7.0.2",
    "cypress": "5.3.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-plugin-react-hooks": "4.1.2",
    "husky": "4.3.0",
    "lint-staged": "10.4.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.1.2"
  }
}
